<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstAdminUse.Admin_CreateNewVendorActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

    <ImageView
        android:id="@+id/addandbacknewuser"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="38dp"
        android:src="@drawable/left_arrow_svgrepo_com__1_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="271dp"
        android:layout_height="226dp"
        android:layout_marginTop="55dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_jygmhx5pdx22zvcjurbrth7orr" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="225dp"
        android:text="Deliver Favourite Food"
        android:textColor="#573201"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="316dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:text="Create New User Admin"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#573201"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="HardcodedText,TextSizeCheck" />

    <EditText
        android:id="@+id/AdminCreateNewVendorName"
        android:layout_width="325dp"
        android:layout_height="57dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/edit_text"
        android:drawablePadding="10dp"
        android:elevation="5dp"
        android:ems="10"
        android:hint="Enter Vendor Name"
        android:inputType="text"
        android:paddingLeft="10dp"
        android:paddingEnd="10dp"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,SpeakableTextPresentCheck,TextContrastCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/AdminCreateNewVendorTypesOfFood"
        android:layout_width="325dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edit_text"
        android:drawablePadding="10dp"
        android:elevation="5dp"
        android:ems="10"
        android:hint="Types Of Foods"
        android:inputType="text"
        android:paddingLeft="10dp"
        android:paddingEnd="10dp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="@+id/AdminCreateNewVendorName"
        app:layout_constraintStart_toStartOf="@+id/AdminCreateNewVendorName"
        app:layout_constraintTop_toBottomOf="@+id/AdminCreateNewVendorName"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/AdminCreateNewVendorAddress"
        android:layout_width="325dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edit_text"
        android:drawablePadding="10dp"
        android:elevation="5dp"
        android:ems="10"
        android:hint="Address"
        android:inputType="text"
        android:paddingLeft="10dp"
        android:paddingEnd="10dp"
        app:layout_constraintEnd_toEndOf="@+id/AdminCreateNewVendorTypesOfFood"
        app:layout_constraintStart_toStartOf="@+id/AdminCreateNewVendorTypesOfFood"
        app:layout_constraintTop_toBottomOf="@+id/AdminCreateNewVendorTypesOfFood"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,SpeakableTextPresentCheck,TextContrastCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/AdminCreateNewVendorImage"
        android:layout_width="0dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edit_text"
        android:drawableRight="@drawable/plus_svgrepo_com__1_"
        android:elevation="5dp"
        android:gravity="center_vertical"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:text="Select Image"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/AdminCreateNewVendorAddress"
        app:layout_constraintStart_toStartOf="@+id/AdminCreateNewVendorAddress"
        app:layout_constraintTop_toBottomOf="@+id/AdminCreateNewVendorAddress" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView880"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="@+id/AdminCreateNewVendorImage"
        app:layout_constraintStart_toStartOf="@+id/AdminCreateNewVendorImage"
        app:layout_constraintTop_toBottomOf="@+id/AdminCreateNewVendorImage">


        <ImageView
            android:id="@+id/AdminCreateVendorImage"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:scaleType="centerCrop"
            android:src="@drawable/addimage" />

    </androidx.cardview.widget.CardView>


            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/AdminCreateNewVendorButton"
                android:layout_width="145dp"
                android:layout_height="50dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/designbutton"
                android:elevation="5dp"
                android:text="Create New Vendor"
                android:textColor="#090606"
                app:layout_constraintEnd_toEndOf="@+id/cardView880"
                app:layout_constraintStart_toStartOf="@+id/cardView880"
                app:layout_constraintTop_toBottomOf="@+id/cardView880"
                tools:ignore="HardcodedText" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>