<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstAdminUse.firstadminsignup">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="271dp"
        android:layout_height="226dp"
        android:layout_marginTop="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_jygmhx5pdx22zvcjurbrth7orr" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="212dp"
        android:text="Deliver Favourite Food"
        android:textColor="#573201"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="394dp"
        android:layout_height="35dp"
        android:layout_marginTop="10dp"
        android:text="Sign Up Here For Your Admin Dashboard"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#573201"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="HardcodedText,TextSizeCheck" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="4dp"
        android:text="Choose Your Location"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout2"
        app:layout_constraintStart_toStartOf="@+id/textInputLayout2"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_editor_absoluteX="20dp"
        app:layout_editor_absoluteY="234dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout2"
        android:layout_width="match_parent"
        android:layout_height="57dp"
        app:endIconDrawable="@drawable/down_arrow_circle_svgrepo_com__1_"
        style="@style/Widget.Material3.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
        app:boxStrokeWidthFocused="0dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="25dp"
        android:layout_marginRight="20dp"
        app:endIconTint="@color/black"
        android:elevation="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <AutoCompleteTextView
            android:id="@+id/Admin_listoflocation"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:inputType="none"
            android:text="Choose Location" />

    </com.google.android.material.textfield.TextInputLayout>

    <EditText
        android:id="@+id/adminnameofOwner"
        android:layout_width="0dp"
        android:layout_height="57dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/edit_text"
        android:drawableEnd="@drawable/profile"
        android:drawablePadding="10dp"
        android:elevation="5dp"
        android:ems="10"
        android:hint="Name Of Owner"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        android:paddingEnd="10dp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="@+id/textInputLayout2"
        app:layout_constraintStart_toStartOf="@+id/textInputLayout2"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout2"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/adminnameofRestaruant"
        android:layout_width="0dp"
        android:layout_height="57dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/edit_text"
        android:drawableEnd="@drawable/home"
        android:drawablePadding="10dp"
        android:elevation="5dp"
        android:ems="10"
        android:hint="Name Of Restorent"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        android:paddingEnd="10dp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="@+id/adminnameofOwner"
        app:layout_constraintStart_toStartOf="@+id/adminnameofOwner"
        app:layout_constraintTop_toBottomOf="@+id/adminnameofOwner"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,SpeakableTextPresentCheck,TouchTargetSizeCheck" />


    <EditText
        android:id="@+id/adminSignupEmail"
        android:layout_width="0dp"
        android:layout_height="57dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/edit_text"
        android:drawableEnd="@drawable/emaillogo111"
        android:drawablePadding="10dp"
        android:elevation="5dp"
        android:ems="10"
        android:hint="E-mail"
        android:inputType="textEmailAddress"
        android:paddingLeft="10dp"
        android:paddingEnd="10dp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="@+id/adminnameofRestaruant"
        app:layout_constraintStart_toStartOf="@+id/adminnameofRestaruant"
        app:layout_constraintTop_toBottomOf="@+id/adminnameofRestaruant"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/adminsignupPassword"
        android:layout_width="0dp"
        android:layout_height="57dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/edit_text"
        android:drawableEnd="@drawable/lockicon111"
        android:drawablePadding="10dp"
        android:elevation="5dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        android:paddingEnd="10dp"
        app:layout_constraintEnd_toEndOf="@+id/adminSignupEmail"
        app:layout_constraintStart_toStartOf="@+id/adminSignupEmail"
        app:layout_constraintTop_toBottomOf="@+id/adminSignupEmail"
        tools:ignore="Autofill,HardcodedText,RtlHardcoded,RtlSymmetry,SpeakableTextPresentCheck,TextContrastCheck,TouchTargetSizeCheck" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/adminSignupCreateaccount"
        android:layout_width="135dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/designbutton"
        android:elevation="5dp"
        android:text="Create Account"
        android:textColor="#090606"
        app:layout_constraintEnd_toEndOf="@+id/adminsignupPassword"
        app:layout_constraintStart_toStartOf="@+id/adminsignupPassword"
        app:layout_constraintTop_toBottomOf="@+id/adminsignupPassword"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/Admin_Alreadyhaveaccount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Already Have Acconut"
        android:textAlignment="center"
        android:textColor="#573201"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/adminSignupCreateaccount"
        app:layout_constraintStart_toStartOf="@+id/adminSignupCreateaccount"
        app:layout_constraintTop_toBottomOf="@+id/adminSignupCreateaccount"
        tools:ignore="HardcodedText,MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>